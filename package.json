        "url": "https://github.com/simonpra/Gutenberg-Columns-Block/issues"
    },
    "homepage": "https://github.com/simonpra/Gutenberg-Columns-Block#readme",
    "devDependencies": {
        "@wordpress/block-editor": "^3.6.0",
        "@wordpress/block-library": "^2.13.0",
        "@wordpress/components": "^9.0.0",
        "@wordpress/scripts": "^6.2.0",
        "bootstrap": "^4.4.1",
        "css-loader": "^3.4.2",
        "lodash": "^4.17.15",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.1",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3"
