{
    "name": "gutenberg-columns-block",
    "version": "0.0.1",
    "description": "Un super bloc Gutenberg pour wordpress permettant d'utiliser les Colonnes de Bootstrap !",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "sass:bootstrap": "node src/scripts/sass ./src/sass/bootstrap.sass",
        "build": "wp-scripts build ./src/index.js --output=./build/js/gutenberg.js",
        "dev": "wp-scripts start ./src/index.js --output=./build/js/gutenberg.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/simonpra/Gutenberg-Columns-Block.git"
    },
